{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/test-type-orm/src",
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true,
        "tsConfigPath": "apps/test-type-orm/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
    },
    "monorepo": true,
    "root": "apps/test-type-orm",
    "projects": {
        "test-type-orm": {
            "type": "application",
            "root": "apps/test-type-orm",
            "entryFile": "main",
            "sourceRoot": "apps/test-type-orm/root",
            "compilerOptions": {
                "tsConfigPath": "apps/test-type-orm/tsconfig.app.json"
            }
        },
        "register": {
            "type": "application",
            "root": "apps/register",
            "entryFile": "main",
            "sourceRoot": "apps/register/root",
            "compilerOptions": {
                "tsConfigPath": "apps/register/tsconfig.app.json"
            }
        }
    }
}